#include <stdio.h>

int main() {
    int A, B, C;
    A = 10;
    B = 20;
    C = 0;
    printf("O valor da variavel A: %d\n",A);
    printf("O valor da variavel B: %d\n",B);
    
    C = A;
    A = B;
    B = C;
    printf("O valor da variavel A: %d\n",A);
    printf("O valor da variavel B: %d\n",B);
 
    return 0;
}

#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int velocidade, distancia, tempo, dias, resto;
velocidade = 278280;
distancia = 570000000;

tempo =distancia/velocidade;
dias = tempo/24;
resto = tempo % 24;
printf("dias %d %d horas\n",dias,resto);

	return 0;
}

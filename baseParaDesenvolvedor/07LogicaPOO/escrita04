class Pessoa {
  private  _nome: string;
  private  _idade: number;
  private  _cpf: string;
  constructor (nome: string, idade: number, cpf: string){
        this._nome = nome;
        this._idade = idade;
        this._cpf = cpf;
  }
  get nome(): string{
    return this._nome;
  }
  get idade(): number{
    return this._idade;
  }
  get cpf(): string{
    return this._cpf;
  }

}
